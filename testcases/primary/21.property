# RFC 5996 (page 10, figure): the first IKE_AUTH response MUST contain the correct IKE version number

{
  "testcases": [
    {
      "testcase": [
        { 
          "receiver": "ue", 
          "name": "enable_vowifi",
          "reporter": "epdg"
        },
        { 
          "receiver": "ue", 
          "name": "ike_sa_init_response",
          "reporter": "epdg"
        },
        { 
          "receiver": "epdg", 
          "name": "ike_auth_1_response",
          "reporter": "epdg",
          "sub": [
            {
              "name": "ike_version",
              "type": "uint8",
              "value": VALUE([0, 16, 32, 255], [32])
            }
          ]
        }
      ]
    }
  ]
